	// }
	"subrank substitution": {
		"prefix": "sub",
		"body": [
			"{",
			"\"aa0\": \"$1\",",
			"\"aa1\": \"$2\",",
			"\"name\": \"$3\" ,",
			"\"position\": $4,",
			"\"c0\": \"$5\",",
			"\"c1\": \"$6\",",
			"\"year\" : $7",
			"}"
		]
		"description": "subrank substitution template"
	},
	"acorgdb antigen cluster": {
